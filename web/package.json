    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": "off",
      "vue/no-unused-vars": "off",
      "no-param-reassign": "off",
      "object-shorthand": "off",
      "quote-props": "off",
      "func-names": "off",
      "no-console": "off",
      "camelcase": "off",
      "no-undef": "off",
      "max-len": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
